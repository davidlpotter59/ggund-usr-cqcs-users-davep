where sfsagent:agent_master_code one of 106, 107, 109, 111, 119, 129, 147,
        154, 157, 161, 165, 166, 170, 176, 177, 211, 267, 710, 733, 756

list
/nobanner
/domain="sfsagent"

sfsagent:agent_no 
sfsagent:agent_master_code 
sfsagent:name[1]

sorted by sfsagent:agent_master_code /newlines 

top of sfsagent:agent_master_code 
box/noblanklines /noheadings 
  sfsagent:agent_master_code 
  sfsagent:name[1]
end box
""/newline
